# Backend API URL
# For development, this defaults to same-origin (empty string means relative URLs)
# For production, set this to your deployed backend URL
REACT_APP_BACKEND_URL=http://localhost:3001

# Site URL for email redirects (used by Supabase)
# This will be set by the deployment agent
REACT_APP_SITE_URL=http://localhost:3000

# Audius API app name for streaming requests
# This is used to identify your app when making requests to the Audius API
REACT_APP_AUDIUS_APP_NAME=spotify-clone
